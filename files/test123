Vagrant.configure("2") do |conf|
	conf.vm.box = "precise32"
	conf.vm.box_url = "http://files.vagrantup.com/precise32.box"
	conf.vm.network :private_network, ip: "192.168.2.4"

	conf.vm.provision "ansible" do |ansible|
	ansible.playbook = "machine2.yaml"
	end

	conf.vm.provider :virtualbox do |vb|
	vb.name = "machine2"
	end

end