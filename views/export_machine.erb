<div class="page-header">
  <h1>Export <%= @machine_name %></h1>
</div>

<div class="row">
  <div class="col-sm-offset-1 col-sm-10">
    <form method="post" action="/<%= @machine_name %>/status.log" class="form-horizontal" enctype="multipart/form-data">


      <div class="form-group">
        <label for="machine_name" class="col-sm-4 control-label">You will export the Machine <%= @machine_name %></label>

      </div>




      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10 text-right">
          <button type="submit" class="btn btn-primary">Download</button>
        </div>
      </div>
    </form>
  </div>
</div>


<script src="import_file_form.js" type="text/javascript"></script>